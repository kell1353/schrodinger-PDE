&integration
	length = 5.0,
    n_points = 100,
    n_steps = 100,
    delta_t = 0.05
/
&wave_function
	width = 2.15,
    center = -0.5,
    potential_scenario = 'harmonic_well'
/
&oscillator
	k_oscillator = 1.0
/
&output
	time_file = 'harmonic_well_2_time_results.dat',
	density_file = 'harmonic_well_2_density_results.dat'
/
